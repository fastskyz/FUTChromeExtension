{
  "name": "FUT Autobuyer",
  "description": "My FUT Autobuyer",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "./images/16.png",
    "32": "./images/32.png",
    "48": "./images/48.png",
    "128": "./images/128.png"
  },
  "background": {
    "scripts": [
      "./js/bg/globals.js",
      "./js/bg/extension.js",
      "./js/bg/handlers.js",
      "./js/bg/index.js"
    ]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://www.ea.com/fifa/ultimate-team/web-app/*"
  ]
}
