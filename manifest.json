{
    "name": "FUT Autobuyer",
    "description": "My FUT Autobuyer",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "16": "./images/16.png",
        "32": "./images/32.png",
        "48": "./images/48.png",
        "128": "./images/logo.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.ea.com/fifa/ultimate-team/web-app/*",
        "https://www.google.com/"
    ]
}